{
  "name": "tsp_solution",
  "strict": true,
  "schema": {
    "type": "object",
    "properties": {
      "solution": {
        "type": "array",
        "description": "巡回セールスマン問題を解決するために辿る経路を表すノードの順序。（条件1:要素の重複不可、条件2:0~nの要素を全て含むこと）",
        "items": {
          "type": "integer",
          "description": "解に含まれる場所の識別子または名称。"
        }
      }
    },
    "required": [
      "solution"
    ],
    "additionalProperties": false
  }
}